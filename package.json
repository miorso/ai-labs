{
  "name": "ai-labs",
  "version": "0.1.0",
  "description": "AI research and learning repository using JavaScript/TypeScript, AI SDK, and React",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\"",
    "type-check": "tsc --noEmit",
    "test": "echo \"Add your test framework (Jest, Vitest, etc.) and update this script\"",
    "validate": "pnpm run format:check && pnpm run lint && pnpm run type-check",
    "release": "standard-version"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "ai-sdk",
    "react",
    "typescript",
    "experiments"
  ],
  "author": "miorso",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.0"
  }
}
